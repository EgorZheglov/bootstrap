{
  "name": "nodejs-lab-bootstrap",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "deploy": "npm install && npm run build && npm run start",
    "build": "rimraf dist && tsc",
    "dev": "nodemon --config nodemon.json src/bootstrap.ts",
    "debug": "nodemon --inspect --config nodemon.json src/bootstrap.ts",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "db:dev": "sh ./db.dev.sh",
    "seed": "ts-node src/db/generator.ts",
    "seed:admin": "ts-node src/db/admin-generator.ts"
  },
  "author": "Denis Zhirkov",
  "license": "MIT",
  "lint-staged": {
    "*.js": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "npx --no-install commitlint --edit",
      "pre-push": "npm test"
    },
    "lint-staged": {
      "*": "prettier --write"
    }
  },
  "dependencies": {
    "@hapi/boom": "^9.1.4",
    "await-to-js": "^3.0.0",
    "axios": "^0.23.0",
    "checksum": "^1.0.0",
